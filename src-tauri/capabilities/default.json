{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "sql:default",
    "fs:default",
    "dialog:default",
    "store:default",
        "shell:default",
        {
          "identifier": "shell:allow-execute",
          "allow": [
            {
              "args": true,
              "cmd": "binaries/rclone",
              "sidecar": true
            }
          ]
        }
      ]
    }
    